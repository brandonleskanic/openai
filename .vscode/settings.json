  "editor.smoothScrolling": true,
  "editor.wordWrap": "on",
  "accessibility.signalOptions.volume": 65,
  "debug.console.lineHeight": 1,
  "editor.fontSize": 13,
  "workbench.colorTheme": "cursor-dark",
